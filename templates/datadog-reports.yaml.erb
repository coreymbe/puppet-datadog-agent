### MANAGED BY PUPPET

---
:datadog_api_key: '<%= @api_key %>'
:api_url: <%= @datadog_site %>
<% if @host -%>
:pe_console: <%= @host %>
<% end -%>
<% if @hostname_extraction_regex -%>
:hostname_extraction_regex: '<%= @hostname_extraction_regex %>'
<% end -%>
<% if @proxy_http -%>
:proxy_http: <%= @proxy_http %>
<% end -%>
<% if @proxy_https -%>
:proxy_https: <%= @proxy_https %>
<% end -%>
<% if @report_fact_tags -%>
:report_fact_tags:
<%- @report_fact_tags.each do |tag| -%>
  - <%= tag %>
  <%- end -%>
<% end -%>
<% if @report_trusted_fact_tags -%>
:report_trusted_fact_tags:
<%- @report_trusted_fact_tags.each do |tag| -%>
  - <%= tag %>
  <%- end -%>
<% end -%>
<% if @puppet_metric_filters -%>
:metric_filters:
<%- @puppet_metric_filters.each do |filter| -%>
  - <%= filter %>
  <%- end -%>
<% end -%>
<% if @pe_event_reporting -%>
:pe_event_types:
<%- @pe_event_types.each do |type| -%>
  - <%= type %>
  <%- end -%>
<% end -%>
